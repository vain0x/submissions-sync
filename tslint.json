{
    "extends": [
        "tslint:recommended"
    ],
    "defaultSeverity": "warn",
    "rulesDirectory": [],
    "rules": {
        "arrow-parens": false,
        "curly": false,
        "interface-name": false,
        "max-classes-per-file": false,
        "member-access": false,
        "member-ordering": false,
        "no-bitwise": false,
        "no-console": false,
        "no-empty": false,
        "no-shadowed-variable": false,
        "object-literal-sort-keys": false,
        "semicolon": [
            true,
            "never"
        ],
        "variable-name": false
    }
}